@startuml

scale 800 width
frame "1" as first {
    card prev1 [
        <b>at - 1
        ====
        prev
        ----
        next
        ----
        data
    ]
    card current1 [
        <b>at
        ====
        prev
        ----
        next
        ----
        data
    ]
    card next1 [
        <b>at + 1
        ====
        prev
        ----
        next
        ----
        data
    ]
    card new1 #palegreen [
        <b>New
        ====
        prev
        ----
        next
        ----
        data
    ]
    prev1 <-r-> current1
    current1 <-r-> next1
    current1 -d- new1 #line:white
}

